      <Button fx:id="buttoneff" layoutX="487.0" layoutY="500.0" mnemonicParsing="false" text="Effacer" />
      <Button fx:id="buttonrec" layoutX="645.0" layoutY="500.0" mnemonicParsing="false" text="Recommencer" />
