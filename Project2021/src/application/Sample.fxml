            <ComboBox fx:id="comboresult" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <ComboBox fx:id="comboservice" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
